{
    "env": {
        "es2021": true,
        "browser": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "import/prefer-default-export": "off",
        "import/no-default-export": "error",
        "no-return-assign": "off",
        "no-param-reassign": "off"
    }
}
